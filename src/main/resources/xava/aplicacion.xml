<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd">

<aplicacion nombre="registrodeaplicaciones"> 
	
	<!--
	Un m�dulo por defecto se asume para cada
	controlador en <modulo-defecto/> 
	-->
	<modulo-defecto>        
        <controlador nombre="CustomTypical"/>        
    </modulo-defecto>

	<modulo nombre="LicenciaDashboard">
		<modelo nombre="LicenciaDashboard"/>
		<controlador nombre="LicenciaDashboard"/>
		<controlador-modo nombre="DetailOnly"/>
  </modulo>
	
	<!--
	Podemos definir m�dulos adicionales, por ejemplo: 	
	<modulo nombre="ProfesorSoloLectura">
		<modelo nombre="Profesor"/>
		<controlador nombre="Print"/>
	</modulo>		
	-->	  
		
</aplicacion>

